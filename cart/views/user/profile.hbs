<div class="row">
    <div class="col-md-4 col-md-offset-4">
        <h1>PROFILE</h1>
        {{#if hasErrors}}
        <div class="alert alert-danger">
            {{#each messages}}
            <p>{{this}}</p>
            {{/each}}
        </div>
        {{/if}}
        <form action="/user/signin/profile" method="post">
            <div class="form-group">
                <label for="">Name</label>
            </div>

            <button class="btn btn-success" type="submit">Logout</button>
        </form>
    </div>
</div>